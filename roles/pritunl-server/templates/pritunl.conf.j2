{
    "mongodb_uri": "{{ pritunl_mongodb_uri }}",
    {% if pritunl_secondary_mongodb_uri|length -%}
    "secondary_mongodb_uri": "{{ pritunl_secondary_mongodb_uri }}",
    {% endif -%}
    {% if pritunl_server_key_path|length %}
    "server_key_path": "{{ pritunl_server_key_path }}",
    {% endif %}
    {% if pritunl_server_cert_path|length %}
    "server_cert_path": "{{ pritunl_server_cert_path }}",
    {% endif %}
    "log_path": "{{ pritunl_log_path }}",
    "static_cache": {{ pritunl_static_cache }},
    "temp_path": "{{ pritunl_temp_path }}",
    "bind_addr": "{{ pritunl_bind_addr }}",
    "port": "{{ pritunl_port }}",
    "debug": {{ pritunl_debug }},
    "www_path": "{{ pritunl_www_path }}",
    "local_address_interface": "{{ pritunl_local_address_interface }}"
}